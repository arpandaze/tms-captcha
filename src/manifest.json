{
  "name": "TMSCaptcha",
  "description": "Extension to autofill captcha on Nepse TMS",
  "version": "0.2.0",
  "author": "Daze <dazehere@yandex.com>",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "background.ts"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "https://*.nepsetms.com.np/login"
      ],
      "js": [
        "content-script.ts",
        "evaluate.ts",
        "data.json"
      ]
    }
  ],
  "web_accessible_resources": [
    "assets/empty.jpg",
    "assets/sample-j.png"
  ]
}
